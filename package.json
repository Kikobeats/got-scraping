{
	"name": "got-scraping",
	"version": "1.0.4",
	"description": "HTTP client made for scraping based on got.",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"src"
	],
	"dependencies": {
		"header-generator": "^0.0.1-beta.2",
		"got": "^11.8.2",
		"http-proxy-agent": "^4.0.1",
		"http2-wrapper": "^2.0.5",
		"https-proxy-agent": "^5.0.0",
		"ow": "^0.23.0"
	},
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"@apify/eslint-config": "^0.1.4",
		"@apify/eslint-config-ts": "^0.1.2",
		"@babel/cli": "^7.14.3",
		"@babel/core": "^7.14.3",
		"@babel/preset-env": "^7.14.4",
		"@babel/preset-typescript": "^7.13.0",
		"body-parser": "^1.19.0",
		"eslint": "^7.0.0",
		"express": "^4.17.1",
		"fs-extra": "^9.1.0",
		"jest": "^26.6.3",
		"jest-extended": "^0.11.5",
		"jsdoc-to-markdown": "^7.0.0",
		"markdown-toc": "^1.2.0",
		"typescript": "^4.3.2"
	},
	"scripts": {
		"build-docs": "npm run build-toc",
		"build": "rm -rf ./build && tsc -p ./src/tsconfig.json",
		"build:types": "tsc --emitDeclarationOnly -p ./src/tsconfig.json",
		"build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --verbose",
		"build-toc": "markdown-toc ./README.md -i",
		"lint": "./node_modules/.bin/eslint ./src ./test --ext .js,.jsx,.ts",
		"lint:fix": "./node_modules/.bin/eslint ./src ./test --ext .js,.jsx,.ts --fix",
		"test": "npm run build && jest --maxWorkers=3 --collect-coverage"
	},
	"author": {
		"name": "Apify",
		"email": "support@apify.com",
		"url": "https://apify.com"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/apify/got-scraping"
	},
	"bugs": {
		"url": "https://github.com/apify/got-scraping/issues"
	}
}
